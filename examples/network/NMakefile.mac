
ROOT=..\..
LDPATH=$(ROOT)\target\release-win-msvc-dll-x64
INCLUDES=/I$(ROOT)\include /I$(ROOT)\deps\msvc\include

link: compile
	link /LTCG /LIBPATH:$(LDPATH) /OUT:network.exe io.lib msvcrt.lib main.obj
compile:
	cl /c /GL /Zl /Ox /DNDEBUG /DUNICODE /DIO_SHARED_LIB  $(INCLUDES) /Fomain.obj  main.cpp